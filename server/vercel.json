{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }, 
      {
        "src": "public/uploads/(.*)",
        "dest": "/public/uploads/$1"
      }
    ]
  }