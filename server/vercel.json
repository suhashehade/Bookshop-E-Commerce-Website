{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
    }
    ],
    "routes": [
      {
        "src": "/public",
        "dest": "public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }